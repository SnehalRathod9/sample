<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="4" name="TestNG Group Test" parallel="methods">
  <test verbose="2" preserve-order="true" name="Test">
        <groups>
            <run>
                <include name = "Search">
                </include>
            </run>
        </groups>
        <classes>
            <class name="Automation.TestNG_SearchGroup">
            </class>
        </classes>
    </test>
 
    <test name="ToDoApp">
        <groups>
            <run>
                <include name = "ToDo">
                </include>
            </run>
        </groups>
        <classes>
            <class name="Automation.TestNG_ToDoGroup">
            </class>
        </classes>
    </test>
</suite>
  